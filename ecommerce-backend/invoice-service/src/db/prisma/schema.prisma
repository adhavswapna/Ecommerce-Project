datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  // <- points to .env DATABASE_URL
}

generator client {
  provider = "prisma-client-js"
}


model Invoice {
  id        String   @id @default(uuid())
  orderId  String   @unique
  userId   String
  fileUrl  String
  createdAt DateTime @default(now())
}
